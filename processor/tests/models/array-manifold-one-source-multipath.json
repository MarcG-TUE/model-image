{
    "canvas": {
        "width": 550,
        "height": 300
    },
    "data": [
        ["parameters", [
            {
                "name": "alpha1",
                "type": "range",
                "initialValue": -20,
                "description": "direction of path one",
                "range": [-90, 90],
                "step": 1
            },
            {
                "name": "alpha2",
                "type": "range",
                "initialValue": 30,
                "description": "direction of path two",
                "range": [-90, 90],
                "step": 1
            },{
                "name": "beta",
                "type": "range",
                "initialValue": 0.33,
                "description": "relative strength of second path",
                "range": [0.05, 2.0],
                "step": 0.01
            }
            ]
        ],
        ["variables", [{
            "name": "responseVector1",
            "value": "SceneData.projectedResponseVector(5,0.5,SceneData.parameters['alpha1'])",
            "updateOn": ["alpha1"],
            "updateOrder": 1
            },{
                "name": "responseVector2",
                "value": "SceneData.projectedResponseVector(5,0.5,SceneData.parameters['alpha2'])",
                "updateOn": ["alpha2"],
                "updateOrder": 2
            },{
                "name": "mpResponseVector",
                "value": "SceneData.addPoint3D(SceneData.variables['responseVector1'],SceneData.scalePoint3D(SceneData.variables['responseVector2'],SceneData.parameters['beta']))",
                "updateOn": ["alpha1","alpha2","beta"],
                "updateOrder": 3
            }
            ]
        ]
    ],
    "elements": [
        ["arrow", {
            "p1": [25,25],
            "p2": [75,200],
            "attributes": {
                "stroke":  "#0000aa",
                "stroke-width": 1.0
            }
        }],
        ["line", {
            "p1": [25,25],
            "p2": [175,100],
            "attributes": {
                "stroke":  "#aa0000",
                "stroke-width": 1.0
            }
        }],
        ["arrow", {
            "p1": [175,100],
            "p2": [125,200],
            "attributes": {
                "stroke":  "#aa0000",
                "stroke-width": 1.0
            }
        }],
        ["lines", {
            "lines": [
                { "p1": [50,200], "p2": [150,200] },
                { "p1": [75,200], "p2": [75,125] },
                { "p1": [125,200], "p2": [125,125] }
            ],
            "attributes": {
                "stroke":  "#000000",
                "stroke-width": 1.0,
                "stroke-dasharray": "8 4"
            }
        }],
        ["arcs", {
            "arcs":[
                {"center": [75,200], "radius": 60, "start-angle": 90, "end-angle": 106},
                {"center": [125,200], "radius": 60, "start-angle": 66, "end-angle": 90}
            ],
            "attributes": {
                "stroke":  "#000000",
                "stroke-width": 0.5,
                "stroke-dasharray": "4 2"
            }
        }],
        ["clip-art", {
            "id": "antenna-array",
            "transform": "translate(75,205)",
            "number-of-antennas": 2,
            "delta": [50,0]
        }],
        ["latex-texts", {
                "texts": [
                    {
                        "text": "x_1[k]",
                        "position": [65,240]
                    },
                    {
                        "text": "x_2[k]",
                        "position": [115,240]
                    },
                    {
                        "text": "\\alpha_1",
                        "position": [60,130]
                    },
                    {
                        "text": "\\alpha_2",
                        "position": [135,130]
                    },
                    {
                        "text": "s[k]",
                        "position": [10,10]
                    }
                ]
        }],
        [ "axis-3d",
            {
                "type": "Perspective8Q",
                "position": [5,5,3.5],
                "target": [0,0,0],
                "fieldOfView": 45,
                "xDom": [-1.5,1.5],
                "yDom": [-1.5,1.5],
                "zDom": [-1.5,1.5],
                "center": [350,150],
                "scale": 650,
                "content": [
                    ["manifold",
                        {
                            "M": 5,
                            "Delta": 0.5,
                            "response-vectors": [],
                            "oneSide": {
                                "side": "back",
                                "axis1": "SceneData.variables['responseVector1']",
                                "axis2": "SceneData.variables['responseVector2']"
                            },
                            "updateOn": ["alpha1","alpha2"],
                            "updateOrder": 10
                        }
                    ],
                    ["span2D", {
                        "axis1": "SceneData.scalePoint3D(SceneData.variables['responseVector1'], 1.15)",
                        "axis2": "SceneData.scalePoint3D(SceneData.variables['responseVector2'], 1.15)",
                        "updateOn": ["alpha1","alpha2"],
                        "updateOrder": 10
                    }],
                    ["manifold",
                        {
                            "M": 5,
                            "Delta": 0.5,
                            "response-vectors": [],
                            "oneSide": {
                                "side": "front",
                                "axis1": "SceneData.variables['responseVector1']",
                                "axis2": "SceneData.variables['responseVector2']"
                            },
                        "updateOn": ["alpha1","alpha2"],
                        "updateOrder": 10
                        }
                    ],
                    ["lines", {
                        "lines": [
                            {
                                "p1": [0,0,0],
                                "p2": [0,0,-1.5]
                            },{
                                "p1": [0,0,0],
                                "p2": [0,1.5,0]
                            },{
                                "p1": [0,0,0],
                                "p2": [1.5,0,0]
                            }
                        ],
                        "attributes": {
                            "stroke":  "#000000",
                            "stroke-width": 1
                        }
                    }],
                    ["lines", {
                        "lines": [
                            {
                                "p1": "SceneData.scalePoint3D(SceneData.variables['responseVector1'], 0.7)",
                                "p2": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], 0.7)"
                            },
                            {
                                "p1": "SceneData.scalePoint3D(SceneData.variables['responseVector2'], 0.7*SceneData.parameters['beta'])",
                                "p2": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], 0.7)"
                            },
                            {
                                "p1": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], -1.7)",
                                "p2": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], 1.7)"
                            }
                        ],
                        "attributes": {
                            "stroke":  "#aa00aa",
                            "stroke-width": 1.0,
                            "stroke-dasharray": "4 1"
                        },
                        "updateOn": ["alpha1","alpha2","beta"],
                        "updateOrder": 15
                    }],
                    ["vector", {
                        "p1": [0,0,0],
                        "p2": "SceneData.variables['responseVector1']",
                        "attributes": {
                            "stroke":  "#0000aa",
                            "stroke-width": 1.5
                        },
                        "updateOn": ["alpha1","alpha2","beta"],
                        "updateOrder": 10
                    }],
                    ["vector", {
                        "p1": [0,0,0],
                        "p2": "SceneData.variables['responseVector2']",
                        "attributes": {
                            "stroke":  "#aa0000",
                            "stroke-width": 1.5
                        },
                        "updateOn": ["alpha1","alpha2","beta"],
                        "updateOrder": 10
                    }],
                    ["vectors", {
                        "vectors": [
                            {
                                "p1": [0,0,0],
                                "p2": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], 0.7)"
                            },
                            {
                                "p1": [0,0,0],
                                "p2": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], -0.7)"
                            },
                            {
                                "p1": [0,0,0],
                                "p2": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], 1.2)"
                            },
                            {
                                "p1": [0,0,0],
                                "p2": "SceneData.scalePoint3D(SceneData.variables['mpResponseVector'], 1.5)"
                            }
                        ],
                        "attributes": {
                            "stroke":  "#aa00aa",
                            "stroke-width": 1.5
                        },
                        "updateOn": ["alpha1","alpha2","beta"],
                        "updateOrder": 10
                    }]
                ]
            }
        ],
        [
            "latex-text",
            {
                "text": "{\\cal A}",
                "position": [375,25],
                "scale": 2.5,
                "color": "#0000ff"
            }
        ],
        [
            "latex-text",
            {
                "text": "{\\bf{}x}[k]",
                "position": [215,135],
                "scale": 1,
                "color": "#aa00aa"
            }
        ],
        [
            "latex-text", {
                "text": "{\\bf{}a}(\\alpha_1)",
                "position": [240,125],
                "scale": 1.0,
                "color": "#0000aa"
            }
        ],
        [
            "latex-text", {
                "text": "{\\bf{}a}(\\alpha_2)",
                "position": [258,203],
                "scale": 1.0,
                "color": "#aa0000"
            }
        ]
    ]
}