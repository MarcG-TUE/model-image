    "data": [
        ["variables", [
            {
                "name": "signal1",
                "value": "Array.from({length:64}, (_,i)=>[0.5+0.5*Math.sin(2*Math.PI*i*8/64),0])"
            },{
                "name": "signal2",
                "value": "Array.from({length:64}, (_,i)=>[(i<=8)?1:0,0])"
            },{
                "name": "signal3",
                "value": "Array.from({length:64}, (_,i)=>[(i==32)?1:Math.sin(2*Math.PI*(i-32)*4/64)/(2*Math.PI*(i-32)*4/64),0])"
            },{
                "name": "transformedSignal1",
                "value": "SceneData.fourierTransform(SceneData.variables.signal1).map(s=>Math.sqrt(s[0]*s[0]+s[1]*s[1]))"
            },{
                "name": "transformedSignal2",
                "value": "SceneData.fourierTransform(SceneData.variables.signal2).map(s=>Math.sqrt(s[0]*s[0]+s[1]*s[1]))"
